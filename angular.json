{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "demo": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/app/css/bootstrap.css",
              "src/app/css/fasthover.css",
              "src/app/css/flexindex.css",
              "src/app/css/font-awesome.css",
              "src/app/css/jquery.countdown.css",
              "src/app/css/popuo-box.css",
              "src/app/css/style.css"
            ],
            "scripts": [
              "/node_modules/jquery/dist/jquery.min.js",
              "src/app/js/bootstrap-3.1.1.min.js",
              "src/app/js/easyResponsiveTabs.js",
              "src/app/js/imagezoom.js",
              "src/app/js/jquery.countdown.js",
              "src/app/js/jquery.flexisel.js",
              "src/app/js/jquery.flexslider.js",
              "src/app/js/jquery.magnific-popup.js",
              
              "src/app/js/jquery.wmuSlider.js",
              "src/app/js/minicart.js",
              "src/app/js/script.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "styles.css",
              "app/css/bootstrap.css",
              "app/css/fasthover.css",
              "app/css/flexslider.css",
              "app/css/font-awesome.css",
              "app/css/jquery.countdown.css",
              "app/css/popuo-box.css",
              "app/css/style.css"
            ],
            "scripts": [
              "app/js/easyResponsiveTabs.js",
              "app/js/imagezoom.js",
              "app/js/jquery.countdown.js",
              "app/js/jquery.flexisel.js",
              "app/js/jquery.flexslider.js",
              "app/js/jquery.magnific-popup.js",
              "app/js/jquery.min.js",
              "app/js/jquery.wmuSlider.js",
              "app/js/minicart.js",
              "app/js/script.js"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "demo"
}